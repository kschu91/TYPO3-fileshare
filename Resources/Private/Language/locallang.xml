    <data type="array">
        <languageKey index="default" type="array">

            <label index="welcome">Welcome</label>
            <label index="welcome.sub">The following files are available for download</label>

            <label index="downloads.fileName">Filename</label>
            <label index="downloads.created">Creation Date</label>
